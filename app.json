{
  "expo": {
    "name": "Elajy",
    "slug": "ElajyPharmacyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "A pharmacy app for finding medicines and managing donations in Sudan",
    "primaryColor": "#49C5B8",
    "icon": "./assets/images/icon-ar-back.png",
    "scheme": "elajypharmacyapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "image": "./assets/images/icon-ar-back.png",
        "prerendered": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-ar-back.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/images/icon-ar-back.png"
      },
      "icon": "./assets/images/icon-ar-back.png",
      "roundIcon": "./assets/images/icon-ar-back.png",
      "edgeToEdgeEnabled": true,
      "package": "com.elajy.elajyapp",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "82ea6619-ce8f-4859-bc31-119edeaf353c"
      }
    }
  }
}
